<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.example.ejerciciowikiclase.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
    <!--Botones que están enlazados a los metodos-->
    <!--Boton para abrir archivo-->
    <Button fx:id="openFileButton" text="Abrir archivo" onAction="#openFile" />
    <!--Boton para guardar archivo-->
    <Button fx:id="saveFileButton" text="Guardar archivo" onAction="#saveFile" />
    <!--Boton para abrir multiples archivos-->
    <Button fx:id="openMultipleFilesButton" text="Abrir múltiples archivos" onAction="#openMultipleFiles" />

</VBox>
